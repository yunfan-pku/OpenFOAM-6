table
{
    #include "thermo"
    outputfilename ThermoTable_CH4_H2O1.txt;
    species (CO2 H2O CH4);

    component_ini (0.90 0.1 0.0001);
    component_fin (0.09 0.01 100);
    num_comp 50;
    pressure_ini  28000000;//30000000;	567.096	129.742
    pressure_fin  70000000;
    num_pre  100;

    temperature_ini 280;//450;//633.15;
    temperature_fin 1200;
    num_temp  100;
    PCFlag 0;//Phase change flag 1 no, 0 yes
    HEFlag 0;//0 H,p; 1 sie,rho
}

