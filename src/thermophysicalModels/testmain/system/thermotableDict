table
{
    #include "thermo"
    outputfilename compre3.plt;
    species (CO2 H2O);//(CO2 H2O CH4);

    component_ini (0.3 0.7);//(0.8 0.2 0.0001);
    component_fin (0.3 0.7);//(0.8 0.2 1000);
    num_comp 0;//50;
    pressure_ini  0.1e+05;//30000000;	567.096	129.742
    pressure_fin  600e+05;
    num_pre  1000;

    temperature_ini 200;//450;//633.15;
    temperature_fin 700;
    num_temp  1000;
    PCFlag 0;//Phase change flag 1 no, 0 yes
    HEFlag 0;//0 H,p; 1 sie,rho
}

PT
{
    #include "thermo"
    outputfilename PTo2.csv;
    species (CO2 H2O CH4);

    component (0.7 0.2 0.1);//(0.488372 0.511628);//(0.290323 0.709677);//(0.929712 0.070288);//(0.012494 0.987506);

    pressure_ini 100000;
    pressure_fin 70000000;
    num_pre  100;

    temperature_ini 200;
    temperature_fin 1200;
    num_temp  100;
}

PX
{
    #include "thermo"
    outputfilename PX.txt;
    species (CO2 H2O);

    component_ini (0 1);
    component_fin (1 0);
    num_comp 100;

    pressure_ini   100000;
    pressure_fin 40000000;
    num_pre  100;

    temperature 392;

}

