TableFlag 0;
PTFlag 0;
PXFlag 1;
phaseFlag 0;

phase
{
    #include "thermo"
    inputfilename H.0.csv;
    outputfilename H.out.csv;
    species (CO2 H2O);
}

table
{
    #include "thermo"
    outputfilename ThermoTable_2D_CO2H2O.txt;
    species (CO2 H2O);

    component_ini (0.5 0.5);
    component_fin (1 0.00001);
    num_comp 0;
    pressure_ini  23000000;//30000000;	567.096	129.742
    pressure_fin 11000000;
    num_pre  0;

    temperature_ini 392;//450;//633.15;
    temperature_fin 600;
    num_temp  0;
    PCFlag 0;//Phase change flag 1 no, 0 yes
    HEFlag 0;//0 H,p; 1 sie,rho
}

PT
{
    #include "thermo"
    outputfilename PT.csv;
    species (CO2 H2O);

    component (0.5 0.5);

    pressure_ini 23000000;
    pressure_fin 70000000;
    num_pre  1;

    temperature_ini 392;
    temperature_fin 600;
    num_temp  1;
}

